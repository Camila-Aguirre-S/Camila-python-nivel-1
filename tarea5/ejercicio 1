import functools


def multiplicar_fun():
    operacion = input("Ingrese una operaci贸n de multiplicaci贸n separada por '*': ")
    numeros = list(map(int, operacion.split('*')))
    resultado = functools.reduce(lambda x, y: x * y, numeros)
    return resultado
print(multiplicar_fun())


