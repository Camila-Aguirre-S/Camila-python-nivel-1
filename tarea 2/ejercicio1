num = int(input("Ingrese un número: ")) 
factorial = 1

if num < 0:
   print("  error ")
elif num == 0 or num == 1:
   print("Factorial de",num,"es 1")
# Si el número es mayor que 1, calcular el factorial
else:
   for i in range(2,num+1):
       factorial *= i
   print("El factorial de",num,"es",factorial)








   


